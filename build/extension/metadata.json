{
  "name": "Portainer",
  "provider": "Portainer.io",
  "vm": {
    "composefile": "docker-compose.yml",
    "exposes": { "socket": "docker.sock" }
  },
  "ui": {
    "dashboard-tab": {
      "title": "Portainer",
      "root": "/",
      "src": "index.html",
      "backend": { "socket": "docker.sock", "tcp": "9000" }
    }
  }
}
